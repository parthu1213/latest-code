!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title Medical
    = csrf_meta_tags
    = csp_meta_tag

    = stylesheet_link_tag 'application', media: 'all'
    = yield :load_page_css
    = yield :css
  %body
    = render "layouts/header"
    = yield
    = javascript_include_tag 'application'
    = yield :load_page_js
    = yield :javascript
