              %tr.info-table-row
                %th.info-table-icon
                  %i.fa.fa.fa-map-marker
                %td.info-table-content 202-204 Panvel Point, Nr. Sudama Chowk
              %tr.info-table-row
                %th.info-table-icon
                  %i.fa.fa.fa-phone
                %td.info-table-content (+91) 70166 76005
