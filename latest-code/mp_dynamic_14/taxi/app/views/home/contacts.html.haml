                %th.info-table-icon
                  %i.fa.fa.fa-fax
                %td.info-table-content (+91) 70166 &lt;76005&gt;76005&gt;
              %tr.info-table-row
                %th.info-table-icon
                  %i.fa.fa.fa-mobile
                %td.info-table-content (+91) 70166 76005
              %tr.info-table-row
                %th.info-table-icon
                  %i.fa.fa.fa-envelope
                %td.info-table-content contact@softices.com
              %tr.info-table-row
                %th.info-table-icon
                  %i.fa.fa.fa-globe
                %td.info-table-content
                  %a{:href => "https://softices.com/", :target => "_blank"} https://softices.com
        .col-lg-6.col-md-6.contact-form
          %form.form.form-validate
            %h3.aligncenter Send Message
            .form-group
              %label
                Your name
                %span.red *
              %input#name.ajaxField.required{:name => "name", :placeholder => "Your name", :type => "text"}/
            .form-group
              %label
                E-mail
                %span.red *
              %input#email.ajaxField.required{:name => "email", :placeholder => "E-mail", :type => "text"}/
            .form-group
              %label
                Message
                %span.red *
              %textarea#text.ajaxField.required{:name => "text", :placeholder => "Enter Message"}
            %input.btn.btn-yellow.aligncenter.btn-lg{:name => "send", :type => "submit", :value => "Send"}/
            %input#type.ajaxField{:name => "type", :type => "hidden", :value => "1"}/
