  %section.hero-wrap.js-fullheight{:style => "background-image: url('#{image_path 'bg_2.jpg'}'); height: 435px;"}
    .overlay
    .container
      .row.no-gutters.slider-text.js-fullheight.align-items-center.justify-content-center{:style => "height: 435px;"}
        .col-md-9.ftco-animate.text-center.fadeInUp.ftco-animated
          %h1.mb-3.bread Contact Us
          %p.breadcrumbs
            %span.mr-2
              %a{:href => root_path} Home
            %span Contact
  %section.ftco-section.contact-section
    .container
      .row.block-9
        .col-md-4.contact-info.ftco-animate.bg-light.p-4.fadeInUp.ftco-animated
          .row
            .col-md-12.mb-4
              %h2.h4 Contact Information
            .col-md-12.mb-3
              %p
                %span> Address:
                202-204 Panvel Point, Nr. Sudama Chowk, Mota Varachha, Surat, Gujarat, India
            .col-md-12.mb-3
              %p
                %span Phone:
                %a{:href => "tel://1234567920"} +91 70166 76005
            .col-md-12.mb-3
              %p
                %span Email:
                %a{:href => "mailto:info@yoursite.com"} contact@softices.com
            .col-md-12.mb-3
              %p
                %span Website:
                %a{:href => "#"} yoursite.com
        .col-md-1
        .col-md-6.ftco-animate.fadeInUp.ftco-animated
          %form.contact-form{:action => "#"}
            .row
              .col-md-6
                .form-group
                  %input.form-control{:placeholder => "Your Name", :type => "text"}/
              .col-md-6
                .form-group
                  %input.form-control{:placeholder => "Your Email", :type => "text"}/
            .form-group
              %input.form-control{:placeholder => "Subject", :type => "text"}/
            .form-group
              %textarea.form-control{:cols => "30", :id => "", :name => "", :placeholder => "Message", :rows => "7"}
            .form-group
              %input.btn.btn-primary.py-3.px-5{:type => "submit", :value => "Send Message"}/
